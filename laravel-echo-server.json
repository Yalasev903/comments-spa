{
  "authHost": "http://localhost:8080",
  "authEndpoint": "/broadcasting/auth",
  "clients": [
    {
      "appId": "local",
      "key": "local"
    }
  ],
  "database": "redis",
  "databaseConfig": {
    "redis": {
      "host": "redis",
      "port": 6379
    }
  },
  "devMode": true,
  "debug": true,
  "host": "0.0.0.0",
  "port": 6001,
  "protocol": "http",
  "socketio": {},
  "subscribers": {
    "http": true,
    "redis": true
  },
  "apiOriginAllow": {
    "allowCors": true,
    "allowOrigin": "*",
    "allowMethods": "GET, POST",
    "allowHeaders": "Origin, Content-Type, Authorization, X-Socket-ID"
  }
}
